




datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id     Int     @id @default(autoincrement())
  username String @unique
  password String
  memes   Meme[]
}

model Meme {
  id     Int    @id @default(autoincrement())
  title  String
  url    String
  user   User   @relation(fields: [userId], references: [id])
  userId Int
}


